{
    "compilerOptions": {
        "module": "CommonJS",
        "moduleResolution": "node",
        "target": "ESNext",
        "lib": [
            "ESNext"
        ],
        "outDir": "out",
        "paths": {
            "@EM-SCRIPT": [
                "./workspace/em.core/em.lang/em-script",
            ],
            "@bob.test/*": [
                "./workspace/bob.pkg/bob.test/*"
            ],
            "@em.hal/*": [
                "./workspace/em.core/em.hal/*"
            ],
            "@em.lang/*": [
                "./workspace/em.core/em.lang/*"
            ],
            "@em.mcu/*": [
                "./workspace/em.core/em.mcu/*"
            ],
            "@em.utils/*": [
                "./workspace/em.core/em.utils/*"
            ],
            "@ti.distro.cc23xx/*": [
                "./workspace/ti.cc23xx/ti.distro.cc23xx/*"
            ],
            "@ti.mcu.cc23xx/*": [
                "./workspace/ti.cc23xx/ti.mcu.cc23xx/*"
            ]
        },
        "sourceMap": true,
        "strict": true,
        "strictPropertyInitialization": false,
        "esModuleInterop": true
    },
    "include": [
        "./workspace/**/*.ts",
        "./workspace/.emscript/*/*.ts",
    ],
    "exclude": [
        "node_modules",
        "junk"
    ]
}